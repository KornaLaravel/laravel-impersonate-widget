{
    "name": "octopyid/laravel-impersonate",
    "description": "Login As A Different User Quickly",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "SupianIDz",
            "email": "supianidz@gmail.com"
        },
         {
            "name": "OctopyID",
            "email": "contact@octopy.id"
        }
    ],
    "require": {
        "php": "^7.1|^7.2|^7.3|^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Octopy\\LaraPersonate\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Octopy\\LaraPersonate\\LaraPersonateServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
