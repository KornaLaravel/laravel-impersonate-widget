{
    "name": "supianidz/laravel-impersonate",
    "description": "Sign in as another user directly",
    "keywords": [
        "sign in",
        "laravel",
        "impersonate"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "SupianIDz",
            "email": "supianidz@gmail.com"
        },
        {
            "name": "Octopy ID",
            "email": "me@octopy.id"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Octopy\\Sudo\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Octopy\\Sudo\\SudoServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
