{
    "name": "octopyid/laravel-impersonate",
    "description": "Login as a different user quickly",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Supian M",
            "email": "supianidz@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "^10.23"
    },
    "require-dev": {
        "orchestra/testbench": "^8.10"
    },
    "autoload": {
        "psr-4": {
            "Octopy\\Impersonate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Octopy\\Impersonate\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "lab404/laravel-impersonate": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Octopy\\Impersonate\\Providers\\ImpersonateServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/testbench package:test"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
