{
    "name": "octopyid/laravel-impersonate",
    "description": "Login as a different user quickly",
    "keywords": [
        "users",
        "laravel",
        "impersonate"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "SupianIDz",
            "email": "supianidz@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.2",
        "orchestra/testbench": "^7.5|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Octopy\\Impersonate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Octopy\\Impersonate\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "lab404/laravel-impersonate": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Octopy\\Impersonate\\ImpersonateServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/testbench package:test"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
