{
    "name": "octopyid/laravel-impersonate",
    "description": "Login as a different user quickly",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "SupianIDz",
            "email": "supianidz@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "ext-sqlite": "*",
        "orchestra/testbench": "6.17."
    },
    "autoload": {
        "psr-4": {
            "Octopy\\LaraPersonate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Octopy\\LaraPersonate\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "lab404/laravel-impersonate": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Octopy\\LaraPersonate\\ImpersonateServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
